<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:cc="using:UltrawideOverlays.CustomControls"
             x:Class="UltrawideOverlays.CustomControls.ResizableImage"
             Background="Transparent">
    <Image Source="{Binding ImageSource, RelativeSource={RelativeSource AncestorType=cc:ResizableImage}}"></Image>
</UserControl>
